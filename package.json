  "scripts": {
    "start": "storybook dev -p 6006 -s public",
    "build": "npm run clean && rollup -c",
    "clean": "rimraf dist/",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
